const byte PACKAGE_ARM_LEFT = 0;const byte PACKAGE_ARM_RIGHT = 1;const unsigned int TEST_VALUE = 813;void setup() {    Serial.begin(9600);    pinMode(LED_BUILTIN, OUTPUT);}void loop() {    delay(900);    digitalWrite(LED_BUILTIN, HIGH);        // header    Serial.write(0xff);    Serial.write(0xff);        Serial.write(PACKAGE_ARM_RIGHT);    Serial.write(TEST_VALUE);    Serial.write(TEST_VALUE >> 8);        delay(100);        digitalWrite(LED_BUILTIN, LOW);}